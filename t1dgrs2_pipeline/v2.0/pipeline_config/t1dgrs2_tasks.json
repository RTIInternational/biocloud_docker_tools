            "--output_result_file",
            "<inputs.output_result_file>",
            "--output_control_file",
            "<inputs.output_control_file>"
        ],
        "outputs": {
            "results_file": {
                "type": "file",
                "value": "<inputs.output_result_file>"
            },
            "control_file": {
                "type": "file",
