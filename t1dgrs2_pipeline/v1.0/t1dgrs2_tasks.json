{
    "download_gvcfs_from_s3": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "source_s3_bucket": {
                "type": "str"
            },
            "s3_access_key": {
                "type": "str"
            },
            "s3_secret_access_key": {
                "type": "str"
            },
            "target_dir": {
                "type": "dir"
            },
            "downloaded_samples": {
                "type": "file"
            },
            "download_limit": {
                "type": "int"
            }
        },
        "cmd": [
            "python",
            "/opt/download_gvcfs_from_s3.py",
            "--source_s3_bucket",
            "<inputs.source_s3_bucket>",
            "--s3_access_key",
            "<inputs.s3_access_key>",
            "--s3_secret_access_key",
            "<inputs.s3_secret_access_key>",
            "--target_dir",
            "<inputs.target_dir>",
            "--downloaded_samples",
            "<inputs.downloaded_samples>",
            "--download_limit",
            "<inputs.download_limit>"
        ],
        "outputs": {
            "downloaded_samples": {
                "type": "file",
                "value": "<inputs.downloaded_samples>"
            },
            "genedx_manifest_path": {
                "type": "file",
                "value": "<inputs.target_dir>genedx_manifest_path.txt"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>s3_download.log"
            }
        }
    },
    "filter_gvcfs_by_consent": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "imported_gvcfs_dir": {
                "type": "dir"
            },
            "consented_gvcfs_dir": {
                "type": "dir"
            },
            "nonconsented_gvcfs_dir": {
                "type": "dir"
            },
            "master_rti_manifest": {
                "type": "file"
            },
            "new_rti_manifest": {
                "type": "file"
            },
            "genedx_manifest_path": {
                "type": "file"
            }
        },
        "cmd": [
            "/opt/filter_gvcfs_by_consent.sh",
            "--imported_gvcfs_dir",
            "<inputs.imported_gvcfs_dir>",
            "--working_dir",
            "<inputs.working_dir>",
            "--consented_gvcfs_dir",
            "<inputs.consented_gvcfs_dir>",
            "--nonconsented_gvcfs_dir",
            "<inputs.nonconsented_gvcfs_dir>",
            "--master_rti_manifest",
            "<inputs.master_rti_manifest>",
            "--new_rti_manifest",
            "<inputs.new_rti_manifest>",
            "--genedx_manifest_path",
            "<inputs.genedx_manifest_path>"
        ],
        "outputs": {
            "genedx_manifest": {
                "type": "file",
                "value": "<inputs.working_dir>genedx_manifest.csv"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>filter_gvcfs_by_consent.log"
            }
        }
    },
    "batch_calculate_t1dgrs2": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "gvcf_dir": {
                "type": "dir"
            },
            "variant_list": {
                "type": "file"
            },
            "genedx_manifest": {
                "type": "file"
            },
            "output_dir": {
                "type": "dir"
            },
            "argo_api_url": {
                "type": "str",
                "default": "http://argo-early-check-rs-1-server:2746/api/v1/workflows/early-check-rs-1"
            },
            "simultaneous_jobs": {
                "type": "int",
                "default": 50
            },
            "control_dir": {
                "type": "dir",
                "default": ""
            },
            "ref_bfile": {
                "type": "str",
                "default": ""
            }
        },
        "cmd": [
            "python",
            "/opt/batch_calculate_t1dgrs2.py",
            "--gvcf_dir",
            "<inputs.gvcf_dir>",
            "--variant_list",
            "<inputs.variant_list>",
            "--genedx_manifest",
            "<inputs.genedx_manifest>",
            "--output_dir",
            "<inputs.output_dir>",
            "--working_dir",
            "<inputs.working_dir>",
            "--argo_api_url",
            "<inputs.argo_api_url>",
            "--simultaneous_jobs",
            "<inputs.simultaneous_jobs>",
            "--control_dir",
            "<inputs.control_dir>",
            "--ref_bfile",
            "<inputs.ref_bfile>"
        ],
        "outputs": {
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>batch_calculate_t1dgrs2.log"
            }
        }
    },
    "extract_gvcf_variants": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "gvcf": {
                "type": "file"
            },
            "out_prefix": {
                "type": "str"
            },
            "variant_list": {
                "type": "file",
                "default": "/t1dgrs2/t1dgrs2_hg19_variants.tsv"
            },
            "pass_only": {
                "type": "int",
                "default": 0
            },
            "filter_by_gq": {
                "type": "int",
                "default": 0
            },
            "hom_gq_threshold": {
                "type": "int",
                "default": 99
            },
            "het_gq_threshold": {
                "type": "int",
                "default": 48
            }
        },
        "cmd": [
            "/opt/extract_gvcf_variants.pl",
            "--gvcf",
            "<inputs.gvcf>",
            "--out_prefix",
            "<inputs.out_prefix>",
            "--variant_list",
            "<inputs.variant_list>",
            "--pass_only",
            "<inputs.pass_only>",
            "--filter_by_gq",
            "<inputs.filter_by_gq>",
            "--hom_gq_threshold",
            "<inputs.hom_gq_threshold>",
            "--het_gq_threshold",
            "<inputs.het_gq_threshold>"
        ],
        "outputs": {
            "t1dgrs2_variants_vcf": {
                "type": "file",
                "value": "<inputs.out_prefix>.vcf"
            },
            "missing": {
                "type": "file",
                "value": "<inputs.out_prefix>.missing"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>extract_gvcf_variants.log"
            }
        }
    },
    "convert_vcf_to_bfile": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "vcf": {
                "type": "file"
            },
            "out_prefix": {
                "type": "str"
            },
            "threads": {
                "type": "int",
                "default": 1
            }
        },
        "cmd": [
            "plink",
            "--vcf",
            "<inputs.vcf>",
            "--out",
            "<inputs.out_prefix>",
            "--make-bed",
            "--threads",
            "<inputs.threads>"
        ],
        "outputs": {
            "bed": {
                "type": "file",
                "value": "<inputs.out_prefix>.bed"
            },
            "bim": {
                "type": "file",
                "value": "<inputs.out_prefix>.bim"
            },
            "fam": {
                "type": "file",
                "value": "<inputs.out_prefix>.fam"
            },
            "plink_log": {
                "type": "file",
                "value": "<inputs.out_prefix>.log"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>convert_vcf_to_bfile.log"
            }
        }
    },
    "merge_bfile_with_reference": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "ref_bfile": {
                "type": "str"
            },
            "sample_bed": {
                "type": "str"
            },
            "sample_bim": {
                "type": "str"
            },
            "sample_fam": {
                "type": "str"
            },
            "out_prefix": {
                "type": "str"
            },
            "threads": {
                "type": "int",
                "default": 1
            }
        },
        "cmd": [
            "plink",
            "--bfile",
            "<inputs.ref_bfile>",
            "--bmerge",
            "<inputs.sample_bed>",
            "<inputs.sample_bim>",
            "<inputs.sample_fam>",
            "--out",
            "<inputs.out_prefix>",
            "--make-bed",
            "--threads",
            "<inputs.threads>"
        ],
        "outputs": {
            "bfile": {
                "type": "str",
                "value": "<inputs.out_prefix>"
            },
            "bed": {
                "type": "file",
                "value": "<inputs.out_prefix>.bed"
            },
            "bim": {
                "type": "file",
                "value": "<inputs.out_prefix>.bim"
            },
            "fam": {
                "type": "file",
                "value": "<inputs.out_prefix>.fam"
            },
            "plink_log": {
                "type": "file",
                "value": "<inputs.out_prefix>.log"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>merge_bfile_with_reference.log"
            }
        }
    },
    "calculate_t1dgrs2": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "bfile": {
                "type": "str"
            },
            "config": {
                "type": "file",
                "default": "/t1dgrs2/t1dgrs2_settings.yml"
            },
            "out_prefix": {
                "type": "str"
            }
        },
        "cmd": [
            "python",
            "-m",
            "t1dgrs2",
            "-b",
            "<inputs.bfile>",
            "-c",
            "<inputs.config>",
            "-o",
            "<inputs.out_prefix>"
        ],
        "outputs": {
            "dosage": {
                "type": "file",
                "value": "<inputs.out_prefix>_dosage.tsv"
            },
            "dq_calls": {
                "type": "file",
                "value": "<inputs.out_prefix>_DQ_calls.tsv"
            },
            "results": {
                "type": "file",
                "value": "<inputs.out_prefix>_RESULTS.tsv"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>calculate_t1dgrs2.log"
            }
        }
    },
    "generate_file_for_export": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "t1dgrs2_results_file": {
                "type": "file"
            },
            "missing_file": {
                "type": "file"
            },
            "sample_id": {
                "type": "str"
            },
            "genedx_manifest": {
                "type": "file"
            },
            "output_file": {
                "type": "str"
            },
            "hla_variants_file": {
                "type": "file",
                "default": "/t1dgrs2/hla_variants.txt"
            },
            "missing_hla_threshold": {
                "type": "int",
                "default": 1
            },
            "non_hla_variants_file": {
                "type": "file",
                "default": "/t1dgrs2/non_hla_variants.txt"
            },
            "missing_non_hla_threshold": {
                "type": "int",
                "default": 3
            }
        },
        "cmd": [
            "/opt/generate_file_for_export.pl",
            "--t1dgrs2_results_file",
            "<inputs.t1dgrs2_results_file>",
            "--missing_file",
            "<inputs.missing_file>",
            "--sample_id",
            "<inputs.sample_id>",
            "--genedx_manifest",
            "<inputs.genedx_manifest>",
            "--output_file",
            "<inputs.output_file>",
            "--hla_variants_file",
            "<inputs.hla_variants_file>",
            "--missing_hla_threshold",
            "<inputs.missing_hla_threshold>",
            "--non_hla_variants_file",
            "<inputs.non_hla_variants_file>",
            "--missing_non_hla_threshold",
            "<inputs.missing_non_hla_threshold>"
        ],
        "outputs": {
            "file_for_export": {
                "type": "file",
                "value": "<inputs.output_file>"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>generate_file_for_export.log"
            }
        }
    },
    "merge_results": {
        "inputs": {
            "working_dir": {
                "type": "dir"
            },
            "output_dir": {
                "type": "dir"
            },
            "sample_results_dir": {
                "type": "dir"
            },
            "output_file": {
                "type": "str"
            }
        },
        "cmd": [
            "/opt/merge_results_files.sh",
            "--sample_results_dir",
            "<inputs.sample_results_dir>",
            "--output_file",
            "<inputs.output_file>"
        ],
        "outputs": {
            "results_file": {
                "type": "file",
                "value": "<inputs.output_file>"
            },
            "log": {
                "type": "file",
                "value": "<inputs.working_dir>merge_results.log"
            }
        }
    },
